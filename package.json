{
  "directories": {
    "example": "examples"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "mocha",
    "build": "npx babel  ./packages/crowded/src -d ./packages/crowded/dist",
    "rollup": "npx rollup ./packages/crowded/dist/Main.js --file crowded.js --format umd --name crowded",
    "release:patch": "npx changelog -p && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\" && npm version patch && git push origin master && git push origin master --tags",
    "_prerelease:patch": "npm run build && npm run rollup"
  },
  "homepage": "https://github.com/crowdedjs/crowded#readme",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "chai": "^4.2.0",
    "generate-changelog": "^1.8.0",
    "mocha": "^8.1.3",
    "rollup": "^2.29.0"
  },
  "version": "1.0.1"
}
